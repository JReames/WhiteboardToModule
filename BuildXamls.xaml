            <ui:InvokeWorkflowFile ContinueOnError="{x:Null}" DisplayName="Invoke BuildAnnotation workflow" sap2010:WorkflowViewState.IdRef="InvokeWorkflowFile_4" UnSafe="False" WorkflowFileName="BuildAnnotation.xaml">
