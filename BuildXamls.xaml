            <ui:InvokeWorkflowFile ContinueOnError="{x:Null}" DisplayName="Invoke BuildArguments workflow" sap2010:WorkflowViewState.IdRef="InvokeWorkflowFile_5" UnSafe="False" WorkflowFileName="BuildArguments.xaml">
